{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Docs Lunetix Cloud",
  "colors": {
    "primary": "#b3b4ff",
    "light": "#a3b4ff",
    "dark": "#4667f8"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "icon": "house",
        "groups": [
          {
            "group": "INTRODUCTION",
            "pages": [
              "index",
              "contacts"
            ]
          },
          {
            "icon": "bags-shopping",
            "group": "Billing",
            "pages": [
              "billing/payment-channels",
              "billing/server-upgrade",
              "billing/server-cancelled"
            ]
          },
          {
            "icon": "user",
            "group": "Users",
            "pages": [
              "users/login",
              "users/personal-details",
              "users/reset-password",
              "users/open-tickets"
            ]
          },
          {
            "icon": "server",
            "group": "Hosting",
            "pages": [
              "hosting/environment-hosting",
              "hosting/minecraft-hosting"
            ]
          },
          {
            "icon": "fingerprint",
            "group": "Essentials",
            "pages": [
              "essentials/privacy-policy",
              "essentials/terms-of-service"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://lunetix.cloud/dashboard"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "discord": "https://discord.com/",
      "github": "https://github.com/VeloxNF",
      "instagram": "https://instagram.com/"
    }
  }
}
